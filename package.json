{
    "name": "learn-tech",
    "version": "1.1.0",
    "description": "An Electron application with React and TypeScript",
    "main": "./out/main/index.js",
    "author": "sebetc4",
    "homepage": "https://electron-vite.org",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint --cache .",
        "lint:fix": "eslint --cache --fix .",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck": "pnpm run typecheck:node && pnpm run typecheck:web",
        "start": "electron-vite preview",
        "dev": "electron-vite dev",
        "build": "pnpm run typecheck && electron-vite build",
        "postinstall": "electron-builder install-app-deps",
        "build:unpack": "pnpm run build && electron-builder --dir",
        "build:win": "pnpm run build && electron-builder --win --publish never",
        "build:mac": "electron-vite build && electron-builder --mac --publish never",
        "build:linux": "electron-vite build && electron-builder --linux --publish never",
        "release:win": "pnpm run build && electron-builder --win --publish always",
        "release:mac": "electron-vite build && electron-builder --mac --publish always",
        "release:linux": "electron-vite build && electron-builder --linux --publish always",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:push": "drizzle-kit push",
        "db:pull": "drizzle-kit pull",
        "db:introspect": "drizzle-kit introspect",
        "db:studio": "drizzle-kit studio"
    },
    "dependencies": {
        "@babel/plugin-transform-modules-commonjs": "^7.27.1",
        "@babel/standalone": "^7.28.5",
        "@electron-toolkit/preload": "^3.0.2",
        "@electron-toolkit/utils": "^4.0.0",
        "@hookform/resolvers": "^5.2.2",
        "@mdx-js/loader": "^3.1.1",
        "@mdx-js/react": "^3.1.1",
        "@radix-ui/react-navigation-menu": "^1.2.14",
        "@types/yauzl": "^2.10.3",
        "check-disk-space": "^3.4.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "drizzle-orm": "^0.44.7",
        "electron-updater": "^6.6.2",
        "embla-carousel-autoplay": "^8.6.0",
        "embla-carousel-react": "^8.6.0",
        "lucide-react": "^0.503.0",
        "parse5": "^7.3.0",
        "radix-ui": "^1.4.3",
        "react-hook-form": "^7.69.0",
        "react-router-dom": "^7.11.0",
        "react-syntax-highlighter": "^15.6.6",
        "recharts": "^3.6.0",
        "sonner": "^2.0.7",
        "sql.js": "^1.13.0",
        "yauzl": "^3.2.0",
        "yup": "^1.7.1",
        "zustand": "^5.0.9"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config-prettier": "^3.0.0",
        "@electron-toolkit/eslint-config-ts": "^3.1.0",
        "@electron-toolkit/tsconfig": "^2.0.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/babel__standalone": "^7.1.9",
        "@types/node": "^25.0.3",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@types/react-syntax-highlighter": "^15.5.13",
        "@types/recharts": "^2.0.1",
        "@types/sql.js": "^1.4.9",
        "@types/tar-stream": "^3.1.4",
        "@vitejs/plugin-react": "^5.1.2",
        "better-sqlite3": "^11.10.0",
        "drizzle-kit": "^0.31.8",
        "electron": "^39.2.7",
        "electron-builder": "^26.0.12",
        "electron-vite": "^5.0.0",
        "eslint": "^9.39.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.26",
        "prettier": "^3.7.4",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "sass": "^1.97.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "vite": "^7.3.0"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@mongodb-js/zstd",
            "@parcel/watcher",
            "better-sqlite3",
            "electron",
            "electron-winstaller",
            "esbuild"
        ]
    }
}
